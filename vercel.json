{
    "projects": {
      "frontend": {
        "root": "frontend"
      },
      "backend": {
        "root": "backend"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)", 
        "destination": "/backend/$1"
      },
      {
        "source": "/(.*)", 
        "destination": "/frontend/$1"
      }
    ]
  }
  